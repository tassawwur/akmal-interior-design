{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "^/landing.html",
      "dest": "/landing.html"
    },
    {
      "src": "^/ur/(.*)",
      "dest": "/ur/$1"
    },
    {
      "src": "^/en/(.*)",
      "dest": "/en/$1"
    },
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "^/(manifest\\.json|favicon\\.ico|robots\\.txt|logo\\d+\\.png)",
      "dest": "/$1"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
} 